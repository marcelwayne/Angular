{
    "name": "aisel/fixture-bundle",
    "type": "symfony-bundle",
    "description": "Fixtures from XML files Symfony2 projects",
    "keywords": ["XML", "fixtures"],
    "homepage": "http://aisel.co",
    "license": "MIT",
    "readmeFilename": "README.md",
    "authors": [
        {
            "name": "Ivan Proskuryakov",
            "email": "volgodark@gmail.com",
            "homepage": "https://github.com/ivanproskuryakov"
        }
    ],
    "require": {
        "php": ">=5.3.3"
    },
    "minimum-stability": "dev",
    "require-dev": {
        "php": ">=5.3.3",

        "symfony/framework-bundle": "~2.3",
        "stof/doctrine-extensions-bundle": "@stable",
        "phpspec/phpspec": "~2.0"
    },
    "autoload": {
        "psr-0": {
            "Aisel\\FixtureBundle": ""
        }
    },
    "target-dir": "Aisel/FixtureBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "0.11-dev"
        }
    }
}