{
    "name": "aisel/config-bundle",
    "type": "symfony-bundle",
    "description": "Settings bundle for Symfony2+SonataAdmin projects.",
    "keywords": ["settings", "configuration", "SonataAdmin"],
    "homepage": "http://aisel.co",
    "license": "MIT",
    "readmeFilename": "README.md",
    "authors": [
        {
            "name": "Ivan Proskuryakov",
            "email": "volgodark@gmail.com",
            "homepage": "https://github.com/ivanproskuryakov"
        }
    ],
    "require": {
        "php": ">=5.3.3"
    },
    "minimum-stability": "dev",
    "require-dev": {
        "php": ">=5.3.3",

        "symfony/framework-bundle": "~2.3",
        "stof/doctrine-extensions-bundle": "@stable",
        "phpspec/phpspec": "~2.0"
    },
    "autoload": {
        "psr-0": {
            "Aisel\\ConfigBundle": ""
        }
    },
    "target-dir": "Aisel/ConfigBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "0.11-dev"
        }
    }
}